{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/search.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/cron_scrape.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/saved_searches.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/details.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "api/search.py"
    },
    {
      "src": "/api/cron",
      "dest": "api/cron_scrape.py"
    },
    {
      "src": "/api/saved",
      "dest": "api/saved_searches.py"
    },
    {
      "src": "/api/details",
      "dest": "api/details.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "*/5 * * * *"
    }
  ],
  "env": {
    "KV_URL": "@kv_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token",
    "POSTGRES_URL": "@postgres_url",
    "CRON_SECRET": "@cron_secret"
  }
}
